{
  "name": "store-management-system",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "seed": "node --require dotenv/config scripts/seed-dev.js",
    "clear-db": "node --require dotenv/config scripts/clear-db.js",
    "migrate-sales": "node --require dotenv/config scripts/migrate-sales-snapshot.js migrate",
    "migrate-sales:dry-run": "node --require dotenv/config scripts/migrate-sales-snapshot.js migrate --dry-run",
    "migrate-sales:rollback": "node --require dotenv/config scripts/migrate-sales-snapshot.js rollback",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --verbose",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=__tests__/unit",
    "test:integration": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=__tests__/integration",
    "test:e2e": "node --experimental-vm-modules node_modules/jest/bin/jest.js --testPathPattern=__tests__/e2e"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "jsonwebtoken": "^9.0.3",
    "lucide-react": "^0.561.0",
    "mongoose": "^9.0.1",
    "next": "^14.2.0",
    "puppeteer": "^24.34.0",
    "react": "^18.3.0",
    "react-day-picker": "^9.12.0",
    "react-dom": "^18.3.0",
    "recharts": "^3.6.0",
    "styled-components": "^6.1.19",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.1",
    "@types/jest": "^29.5.12",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "mongodb-memory-server": "^10.1.2",
    "prettier": "^3.7.4",
    "supertest": "^7.0.0"
  }
}
